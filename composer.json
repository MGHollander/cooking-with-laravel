{
    "name": "mghollander/cooking-with-laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "ext-dom": "*",
        "artesaos/seotools": "^1.3",
        "brick/structured-data": "^0.1.1",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "openai-php/client": "^0.6.2",
        "protonemedia/laravel-cross-eloquent-search": "^3.2",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-sluggable": "^3.5",
        "spatie/laravel-tags": "^4.4",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.3",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.2",
        "laravel/breeze": "^1.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.6",
        "phpunit/phpunit": "^10.2",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-error-solutions": "^0.0.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
