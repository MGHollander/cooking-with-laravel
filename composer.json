{
  "name": "mghollander/cooking-with-laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "ext-dom": "*",
    "ext-imagick": "*",
    "artesaos/seotools": "^1.3",
    "astrotomic/laravel-translatable": "^11.16",
    "brick/structured-data": "^0.2",
    "bugsnag/bugsnag-laravel": "^2.0",
    "guzzlehttp/guzzle": "^7.2",
    "hidehalo/nanoid-php": "^2.0",
    "inertiajs/inertia-laravel": "^2.0",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.7",
    "openai-php/client": "^0.18",
    "patrickschur/language-detection": "^5.3",
    "protonemedia/laravel-cross-eloquent-search": "^3.2",
    "spatie/laravel-medialibrary": "^11.0",
    "spatie/laravel-sluggable": "^3.5",
    "spatie/laravel-tags": "^4.4",
    "tightenco/ziggy": "^2.5.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3.5",
    "deployer/deployer": "^7.3",
    "fakerphp/faker": "^1.9",
    "itsgoingd/clockwork": "^5.2",
    "laravel/boost": "^1.1",
    "laravel/breeze": "^2.3",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.16",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^8.5",
    "phpunit/phpunit": "^12.0",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-error-solutions": "^1.0",
    "spatie/laravel-ignition": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Support/RouteHelper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "fix": [
      "vendor/bin/pint"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
